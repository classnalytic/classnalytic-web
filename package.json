{
  "name": "classnalytic-frontend",
  "version": "1.0.0",
  "scripts": {
    "dev": "nodemon server.js",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "test": "standard --verbose | snazzy",
    "fix": "standard --verbose --fix | snazzy",
    "pkg": "pkg . --targets node8-alpine-x64 --out-path pkg"
  },
  "dependencies": {
    "antd": "^3.8.1",
    "axios": "^0.18.0",
    "babel-plugin-import": "^1.8.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-wrap-in-js": "^1.1.1",
    "express": "^4.16.3",
    "formik": "^1.0.3",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "moment": "^2.22.2",
    "next": "^7.0.0",
    "next-images": "^0.10.6",
    "next-redux-wrapper": "^2.0.0",
    "qrcode.react": "^0.8.0",
    "raw-loader": "^0.5.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0",
    "react-qr-reader": "^2.1.0",
    "react-redux": "^5.0.1",
    "react-webcam": "^1.0.4",
    "recompose": "^0.28.2",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.0.2",
    "victory": "^30.5.0",
    "yup": "^0.26.2"
  },
  "license": "ISC",
  "nodemonConfig": {
    "ignore": [
      ".next/*",
      "node_modules/*"
    ],
    "watch": [
      "server.js"
    ]
  },
  "bin": "server.js",
  "pkg": {
    "assets": [
      ".next/**/*"
    ],
    "scripts": [
      ".next/dist/**/*.js"
    ]
  },
  "standard": {
    "globals": [
      "FormData",
      "File",
      "atob"
    ],
    "parser": "babel-eslint"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-plugin-styled-components": "^1.5.1",
    "http-proxy-middleware": "^0.18.0",
    "nodemon": "^1.18.3",
    "pkg": "^4.3.4",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1"
  }
}
